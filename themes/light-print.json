    "tokenColors": [
        {
            "settings": {
                "foreground": "#262626"
            }
        },
        {
            "scope": [
                "meta.embedded",
                "source.groovy.embedded",
                "string meta.image.inline.markdown",
                "variable.legacy.builtin.python"
            ],
            "settings": {
                "foreground": "#262626"
            }
        },
        {
            "name": "Comment",
            "scope": "comment",
            "settings": {
                "foreground": "#777B90"
            }
        },
        {
            "name": "String",
            "scope": "string",
            "settings": {
                "foreground": "#c55a11"
            }
        },
        {
            "name": "parameters end bracket",
            "scope": "punctuation.definition.parameters.end.bracket.round",
            "settings": {
                "foreground": "#b77da7"
            }
        },
        {
            "name": "parameters begin bracket",
            "scope": "punctuation.definition.parameters.begin.bracket.round",
            "settings": {
                "foreground": "#b77da7"
            }
        },
        {
            "name": "arguments end bracket",
            "scope": "punctuation.definition.arguments.end.bracket.round",
            "settings": {
                "foreground": "#6497b2"
            }
        },
        {
            "name": "arguments begin bracket",
            "scope": "punctuation.definition.arguments.begin.bracket.round",
            "settings": {
                "foreground": "#6497b2"
            }
        },
        {
            "name": "array bracket []",
            "scope": "punctuation.section.array",
            "settings": {
                "foreground": "#19648d"
            }
        },
        {
            "name": "Template Definition",
            "scope": [
                "punctuation.definition.template-expression",
                "punctuation.section.embedded"
            ],
            "settings": {
                "foreground": "#c00000"
            }
        },
        {
            "name": "Reset JavaScript string interpolation expression",
            "scope": [
                "meta.template.expression"
            ],
            "settings": {
                "foreground": "#262626"
            }
        },
        {
            "name": "Number",
            "scope": "constant.numeric",
            "settings": {
                "foreground": "#4c206d"
            }
        },
        {
            "name": "Built-in constant",
            "scope": "constant.language",
            "settings": {
                "foreground": "#4c206d"
            }
        },
        {
            "name": "User-defined constant",
            "scope": "constant.character, constant.other",
            "settings": {
                "foreground": "#4c206d"
            }
        },
        {
            "name": "Variable",
            "scope": "variable",
            "settings": {
                "fontStyle": "",
                "foreground": "#262626"
            }
        },
        {
